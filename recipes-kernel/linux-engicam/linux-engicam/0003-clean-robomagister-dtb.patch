From 6b6819273ae73e41bec237d9d1ead1d8f6cff586 Mon Sep 17 00:00:00 2001
From: Matteo Lisi <matteo.lisi@engicam.com>
Date: Fri, 5 Apr 2024 10:40:01 +0200
Subject: [PATCH] clean robomagister dtb

---
 .../imx8mp-icore-fasteth-robomagister.dts     | 44 ++++++++-----------
 1 file changed, 18 insertions(+), 26 deletions(-)

diff --git a/arch/arm64/boot/dts/engicam/imx8mp-icore-fasteth-robomagister.dts b/arch/arm64/boot/dts/engicam/imx8mp-icore-fasteth-robomagister.dts
index 83742be22331..86d47f742257 100644
--- a/arch/arm64/boot/dts/engicam/imx8mp-icore-fasteth-robomagister.dts
+++ b/arch/arm64/boot/dts/engicam/imx8mp-icore-fasteth-robomagister.dts
@@ -1,6 +1,6 @@
 // SPDX-License-Identifier: (GPL-2.0+ OR MIT)
 /*
- * Copyright 2023 Engicam
+ * Copyright 2024 Engicam
  */
 
 /dts-v1/;
@@ -11,6 +11,15 @@
 / {
 	model = "Engicam i.MX8MPlus Fasteth ";
 
+	chosen {
+		stdout-path = &uart2;
+	};
+
+	aliases {
+		rtc1 = &snvs_rtc;
+	};
+
+
 	wifi_pwrseq: wifi_pwrseq {
 		compatible = "mmc-pwrseq-simple";
 		reset-gpios = <&gpio1 5 GPIO_ACTIVE_LOW>;
@@ -253,8 +262,6 @@
 	};
 };
 
-
-
 &A53_0 {
 	cpu-supply = <&buck2>;
 };
@@ -410,7 +417,11 @@
 		reg = <0x68>;
 	};
 
-
+	mcp3021@4d {
+		compatible = "microchip,mcp3021";
+		reg = <0x4d>;
+		reference-voltage-microvolt = <4500000>; /* 4.5 V */
+	};
 };
 
 &i2c3 {
@@ -463,6 +474,7 @@
 	status = "okay";
 };
 
+
 &easrc {
 	fsl,asrc-rate  = <48000>;
 	status = "okay";
@@ -699,9 +711,9 @@
 		>;
 	};
 
-	pinctrl_panasonic: lairdgrp {
+	pinctrl_panasonic: panasonicgrp {
 		fsl,pins = <
-			MX8MP_IOMUXC_GPIO1_IO05__GPIO1_IO05	0x41 /* WL_REG_ON */
+			MX8MP_IOMUXC_GPIO1_IO05__GPIO1_IO05	0x41
 		>;
 	};
 
@@ -776,18 +788,6 @@
 			MX8MP_IOMUXC_GPIO1_IO02__WDOG1_WDOG_B	0xc6
 		>;
 	};
-
-	pinctrl_csi0_pwn: csi0_pwn_grp {
-		fsl,pins = <
-			MX8MP_IOMUXC_SD2_WP__GPIO2_IO20	0x19
-		>;
-	};
-
-	pinctrl_csi0_rst: csi0_rst_grp {
-		fsl,pins = <
-			MX8MP_IOMUXC_SD1_DATA5__GPIO2_IO07	0x19
-		>;
-	};
 };
 
 
@@ -823,11 +823,3 @@
 	status = "okay";
 };
 
-&i2c2 {
-	mcp3021@4d {
-        compatible = "microchip,mcp3021";
-        reg = <0x4d>;
-        reference-voltage-microvolt = <4500000>; /* 4.5 V */
-	};
-};
